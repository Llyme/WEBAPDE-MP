<html>

<head>
	<title>LuLgag</title>
	<link rel="stylesheet" type="text/css" href="assets/css/style.css">
	<script src=assets/js/qTiny.js></script>
	<script src=assets/js/rTiny.js></script>
</head>

<body>
	<header>
		<div class="flexcontainer">
			<img src="https://i.imgur.com/2EiDBoG.jpg" alt="logo" />
			<h1>LuLgag</h1>
			<button class="up_btn">Upload a meme</button>
			<input type="text" placeholder="search LuLgag..">
		</div>    
	</header>
	<div id=sidebar>
		<div id=profile>
			<img id=profilepicture src=dat/avatar/{{userid}}.jpg>
			<label id=profilename>{{username}}</label>
			<a id=newpost class=btn>Post</a>
			<a id=myposts class=btn href="">My Posts</a>
			<a id=myaccount class=btn href="">My Account</a>
			<a id=logout class=btn>Logout</a>
		</div>
	</div>

	<div id=deck>
		<a id=card href="">
			<label class=poster>Posted by Wowza</label>
			<label class=rep>123</label>
			<label class=header>Hello Man</label>
